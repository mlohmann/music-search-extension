{
  "manifest_version": 3,
  "name": "Recommendation Music Search",
  "version": "1.0",
  "description": "Search your preferred music platforms with a right click.",
  "permissions": ["contextMenus", "activeTab", "scripting", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html"
}
